@using DVG.WIS.Utilities.Serialization;
@using DVG.WIS.Core.Constants;
@using DVG.WIS.Core.Enums;
@{

}
<style>
    .label-order {
        font-weight: bold;
        font-size: 14px;
    }

    .pd-left-30 {
        padding-left: 30px;
    }

    .text-danger {
        font-size: 14px;
    }

    .box-header {
        padding-bottom: 0px;
    }

    .ln_solid {
        margin: 10px 0;
    }

    .mg-right-0 {
        margin-right: 0px;
    }

    .text-wrap {
        font-weight: bold;
        font-size: 16px;
    }
</style>
<script src="~/Scripts/SEO.js"></script>
<script src="~/JsModel/OrderActionController.js"></script>
<section id="updateOrder" class="content" ng-app="OrderAction" ng-controller="OrderActionController" ng-init="Init(@NewtonJson.Serialize(Model))">
    <div class="row">
        <div class="col-md-12 col-sm-12 col-xs-12">
            <h2 class="no-mg-top" style="margin-left: 10px;">@ViewBag.Title</h2>
            @*<h3 ng-if="Order.OrderId > 0">ID : {{Order.OrderId }}</h3>*@
        </div>
    </div>
    <div class="ln_solid" style="margin: 10px; border-bottom: 1px solid #f39c12; margin-top: 0px;"></div>
    <div class="clearfix"></div>
    <form id="frmUpdateOrder" name="frmUpdateOrder" data-parsley-validate="" class="" novalidate="" ng-model="Order">
        <div class="box" style="margin-left: 10px; margin-right: 10px;">
            <div class="box-header">
                <div class="box-title">
                    <small class="text-wrap no-mg-top">Thông tin đơn hàng</small>
                    <div class="clearfix"></div>
                </div>
                <div class="ln_solid"></div>

                <div class="clearfix"></div>
            </div>
            <div class="box-body">
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-xs-6 pd-left-30 mg-bottom-15">
                        <div class="row mg-bottom-5"><span class="label-order mg-right-5"><i class="fa fa-car"></i> Hình thức giao hàng <span class="required text-danger">*</span> </span></div>
                        <div class="row mg-bottom-5 mg-right-0" style="margin-left: 15px;">
                            @*<select class="form-control input-sm" id="OrderType" name="OrderType" required="required"
                                        ng-model="Order.OrderType"
                                        ng-change="onChangeOrderType()" convert-to-number>
                                    <option value="-1" class="" selected="selected">--Lựa chọn--</option>
                                    <option ng-repeat="item in Order.ListOrderType" value="{{item.Id}}">
                                        {{item.Name}}
                                    </option>
                                </select>*@
                            <div ng-repeat="item in Order.ListOrderType">
                                <label style="font-weight: normal;">
                                    <input type="radio" ng-model="Order.OrderType" ng-value="item.Id">
                                    {{item.Name}}
                                </label><br />
                            </div>

                            <div class="mg-bottom-5" ng-show="frmUpdateOrder.OrderType.$error.required || Order.OrderType < 0">
                                <span class="text-danger">Vui lòng chọn hình thức giao hàng.</span>
                            </div>
                            <input type="hidden" value="@((int)OrderTypeEnum.TakeAway)" id="hddOrderTypeTakeAway" />
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-xs-6 pd-left-30 mg-bottom-15">
                        <div class="row mg-bottom-5"><span class="label-order mg-right-5"><i class="fa fa-clock-o"></i> Thời gian giao hàng <span class="required text-danger">*</span></span></div>
                        <div class="row mg-bottom-5 mg-right-0">
                            <input id="DeliverDateStr" name="DeliverDateStr" type="text" required="required" class="form-control datepicker-inp" data-parsley-id="1" ng-model="Order.DeliverDateStr" ng-change="onChangeDeliveryDate($event)" ng-keypress="onChangeDeliveryDate($event)">
                        </div>
                        <div class="mg-bottom-5" ng-show="frmUpdateOrder.SourceType.$error.required">
                            <span class="text-danger">Vui lòng chọn thời gian giao hàng.</span>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-xs-6 pd-left-30 mg-bottom-15">
                        <div class="row mg-bottom-5"><span class="label-order mg-right-5"><i class="fa fa-file-o"></i> Nguồn đơn hàng <span class="required text-danger">*</span> </span></div>
                        <div class="row mg-bottom-5 mg-right-0">
                            <select class="form-control input-sm" id="SourceType" name="SourceType" required="required"
                                    ng-model="Order.SourceType"
                                    ng-change="onChangeSourceType()" convert-to-number>
                                <option value="-1" class="" selected="selected">--Lựa chọn--</option>
                                <option ng-repeat="item in Order.ListSourceType" value="{{item.Id}}">
                                    {{item.Name}}
                                </option>
                            </select>
                            <div class="mg-bottom-5" ng-show="frmUpdateOrder.SourceType.$error.required || Order.SourceType < 0">
                                <span class="text-danger">Vui lòng chọn nguồn đơn hàng.</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-xs-6 pd-left-30 mg-bottom-15">
                        <div class="row mg-bottom-5"><span class="label-order mg-right-5"><i class="fa fa-comment"></i> Ghi chú </span></div>
                        <div class="row mg-bottom-5 mg-right-0">
                            <textarea id="CustomerNote" name="CustomerNote" class="form-control" data-parsley-id="1" ng-model="Order.CustomerNote" ng-maxlength="500" placeholder="Nhập ghi chú cho đơn hàng này"></textarea>
                            <div class="mg-bottom-5" ng-show="frmUpdateOrder.CustomerNote.$error.maxlength">
                                <span class="text-danger">Ghi chú không được quá 500 ký tự.</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div id="customerInfo" class="box" style="margin-left: 10px; margin-right: 10px;">
            <div class="box-header">
                <div class="box-title">
                    <small class="text-wrap no-mg-top">Thông tin khách hàng</small>
                    <div class="clearfix"></div>
                </div>
                <div class="ln_solid my-ln_solid"></div>

                <div class="clearfix"></div>
            </div>
            <div class="box-body">
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-xs-6 pd-left-30 mg-bottom-15">
                        <div class="row mg-bottom-5"><span class="label-order mg-right-5"><i class="fa fa-user"></i> Tên khách hàng <span class="required text-danger">*</span> </span></div>
                        <div class="row mg-bottom-5 mg-right-0">
                            <input type="text" id="CustomerName" name="CustomerName" required="required" class="form-control" data-parsley-id="1" ng-model="Order.CustomerName" ng-maxlength="255" ng-blur="blurCustomerInput()" placeholder="Nhập tên khách hàng">
                            <div class="mg-bottom-5" ng-show="frmUpdateOrder.CustomerName.$error.required">
                                <span class="text-danger">Vui lòng nhập tên khách hàng.</span>
                            </div>
                            <div class="mg-bottom-5" ng-show="frmUpdateOrder.CustomerName.$error.maxlength">
                                <span class="text-danger">Tên khách hàng không được quá 255 ký tự.</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-xs-6 pd-left-30 mg-bottom-15">
                        <div class="row mg-bottom-5"><span class="label-order mg-right-5"><i class="fa fa-phone-square"></i> SĐT <span class="required text-danger">*</span> </span></div>
                        <div class="row mg-bottom-5 mg-right-0">
                            <input type="text" id="CustomerPhone" name="CustomerPhone" required="required" class="form-control" data-parsley-id="1" ng-model="Order.CustomerPhone" ng-maxlength="20" ng-change="onChangeMobileOrder()" ng-blur="blurMobileInput()" placeholder="Nhập số điện thoại">
                            <div ng-show="frmUpdateOrder.CustomerPhone.$error.required">
                                <span class="text-danger">Vui lòng nhập số điện thoại.</span>
                            </div>
                            <div class="mg-bottom-5" ng-show="frmUpdateOrder.CustomerPhone.$error.maxlength">
                                <span class="text-danger">Số điện thoại không được quá 20 ký tự.</span>
                            </div>
                            <div ng-show="Order.CustomerPhone.length > 0 && IsErrorPhone">
                                <span class="text-danger">Số điện thoại không hợp lệ.</span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-lg-6 col-md-6 col-xs-6 pd-left-30 mg-bottom-15">
                        <div class="row mg-bottom-5"><span class="label-order mg-right-5"><i class="fa fa-map-marker"></i> Quận/Huyện <span ng-if="Order.OrderType <= 0" class="required text-danger">*</span> </span></div>
                        <div class="row mg-bottom-5 mg-right-0">
                            <select class="form-control input-sm" id="district" name="district" ng-required="Order.OrderType <= 0"
                                    ng-model="Order.DistrictId"
                                    ng-change="onChangeDistrict()" convert-to-number>
                                <option value="0" rel="0">--Lựa chọn--</option>
                                <option ng-repeat="item in Order.ListDistrict" value="{{item.district_id}}" rel="{{item.ship_fee}}">
                                    {{item.district_name}}
                                </option>
                            </select>
                            <div class="mg-bottom-5" ng-show="frmUpdateOrder.DistrictId.$error.required || (Order.OrderType <= 0 && Order.DistrictId <= 0)">
                                <span class="text-danger">Vui lòng chọn quận/huyện.</span>
                            </div>
                        </div>
                    </div>
                    <div class="col-lg-6 col-md-6 col-xs-6 pd-left-30 mg-bottom-15">
                        <div class="row mg-bottom-5"><span class="label-order mg-right-5"><i class="fa fa-map-marker"></i> Phường/Xã </span></div>
                        <div class="row mg-bottom-5 mg-right-0">
                            <select class="form-control input-sm" id="WardId" name="WardId" ng-required="checkOrderType();"
                                    ng-model="Order.WardId"
                                    ng-change="onChangeWard()" convert-to-number>
                                <option value="0">--Lựa chọn--</option>
                                <option ng-repeat="item in Order.ListWard" value="{{item.ward_id}}">
                                    {{item.ward_name}}
                                </option>
                            </select>
                            @*<div class="mg-bottom-5" ng-show="frmUpdateOrder.WardId.$error.required || Order.WardId <= 0">
                                    <span class="text-danger">Vui lòng chọn phường/xã.</span>
                                </div>*@
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="row pd-left-30"><span class="label-order mg-right-5"><i class="fa fa-location-arrow"></i> Địa chỉ giao hàng <span ng-if="Order.OrderType <= 0" class="required text-danger">*</span> </span></div>
                    <div class="row pd-left-30 mg-right-15">
                        <input type="text" id="DeliveryAddress" name="DeliveryAddress" ng-required="Order.OrderType <= 0" class="form-control" data-parsley-id="1" ng-model="Order.DeliveryAddress" ng-maxlength="255" placeholder="Chi tiết địa chỉ (Ví dụ: số nhà, hẻm...)" />
                        <div class="mg-bottom-5" ng-show="frmUpdateOrder.DeliveryAddress.$error.required || (Order.OrderType <= 0 && Order.DeliveryAddress == '')">
                            <span class="text-danger">Vui lòng nhập địa chỉ.</span>
                        </div>
                        <div class="mg-bottom-5" ng-show="frmUpdateOrder.DeliveryAddress.$error.maxlength">
                            <span class="text-danger">Địa chỉ không được quá 255 ký tự.</span>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12 col-sm-12 col-xs-12">
                <div class="col-md-6 col-sm-6 col-xs-6">
                    <div class="box">
                        <div class="box-header">
                            <div class="box-title">
                                <small class="text-wrap no-mg-top">Danh sách sản phẩm</small>
                                <div class="clearfix"></div>
                            </div>
                            <div class="ln_solid"></div>
                        </div>
                        <div class="box-body">
                            <div class="row mg-bottom-10">
                                <div class="col-md-9 col-sm-9 col-xs-9">
                                    <input type="text" class="form-control input-sm" name="Search.Keyword" ng-keypress="keypressAction($event)" ng-model="Search.KeyWord" placeholder="Tên/Mã sản phẩm">
                                </div>
                                <div class="col-md-2 col-sm-2 col-xs-2">
                                    <button style="width: auto" type="button" class="form-control btn btn-primary btn-sm" ng-click="doSearch()"><i class="fa fa-search"></i></button>
                                </div>
                            </div>
                            <table class="table table-striped table-bordered table-responsive" style="font-size: 14px;">
                                <thead>
                                    <tr role="row">
                                        <th>#</th>
                                        <th>Mã món</th>
                                        <th>Tên món</th>
                                        <th>Đơn giá gốc</th>
                                        <th>Đơn giá sau KM</th>
                                        <th class="text-center">Thao tác</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr ng-if="Order.lstOrderDetailViewModel == null || Order.lstOrderDetailViewModel == []">
                                        <td colspan="8">
                                            <p class="text-center">Không tìm thấy món ăn nào</p>
                                        </td>
                                    </tr>
                                    <tr role="row" ng-repeat="item in Order.lstOrderDetailViewModel track by $index">
                                        <td>
                                            {{((page.currentPage - 1) * page.itemsPerPage) + $index+1}}
                                        </td>
                                        <td>
                                            {{ item.Code }}
                                        </td>
                                        <td> {{ item.ProductName }} </td>
                                        <td> {{ item.OriginPriceStr }} </td>
                                        <td> {{ item.PriceStr }} </td>
                                        <td class="text-center">
                                            <a href="javascript:void(0)" ng-click="insertProduct(item)" class="btn btn-primary btn-sm" data-toggle="tooltip" data-placement="top" title="Thêm vào giỏ hàng"><i class="fa fa-plus"></i> </a>
                                        </td>
                                        @*<td type="hidden" value="{{item.ProductId}}"></td>*@
                                    </tr>

                                </tbody>
                            </table>
                            <div class="row pd-left-10">
                                <div class="col-sm-12">
                                    <div class="dataTables_info mg-top-20 mg-bottom-5" role="status" aria-live="polite">
                                        Bạn đang ở trang {{page.currentPage}}/{{roundNumberPaging(page.itemsPerPage,page.totalItems)}} (có tổng {{page.totalItems}} bản ghi)
                                    </div>
                                </div>
                                <div class="col-sm-12" ng-if="page.totalItems > page.itemsPerPage">
                                    <div class="dataTables_paginate paging_simple_numbers">
                                        <uib-pagination class="pagination pull-right mg-right-10 mg-bottom-10" total-items="page.totalItems" max-size="5" items-per-page="page.itemsPerPage" ng-model="page.currentPage" ng-change="getListData()" previous-text="&lsaquo;" next-text="&rsaquo;" first-text="&laquo;" last-text="&raquo;" boundary-links="true"></uib-pagination>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                </div>
                <div class="col-md-6 col-sm-6 col-xs-6">
                    <div class="box">
                        <div class="box-header">
                            <div class="box-title">
                                <small class="text-wrap no-mg-top">Giỏ hàng của bạn</small>
                                <div class="clearfix"></div>
                            </div>
                            <div class="clearfix"></div>
                            <div class="ln_solid"></div>

                            <div class="clearfix"></div>
                        </div>
                        <div class="box-body">
                            <div style="max-height: 545px; overflow: auto">
                                <table class="table table-striped table-bordered table-responsive" style="font-size: 14px;">
                                    <thead>
                                        <tr role="row">
                                            <th>#</th>
                                            <th>Mã món</th>
                                            <th>Tên món</th>
                                            <th width="20%">Số lượng</th>
                                            <th>Đơn giá gốc</th>
                                            <th>Đơn giá sau KM</th>
                                            <th>Ghi chú</th>
                                            <th class="text-center">Thao tác</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr ng-if="Order.ListProductViewModel == null || Order.ListProductViewModel == [] || Order.ListProductViewModel.length == 0">
                                            <td colspan="8">
                                                <p class="text-center">Không tìm thấy món ăn nào. Vui lòng thêm món ăn vào giỏ hàng</p>
                                            </td>
                                        </tr>
                                        <tr role="row" ng-repeat="item in Order.ListProductViewModel track by $index" id="listOrderDetail">
                                            <td class="indexitem"> {{ $index + 1 }} </td>
                                            <td class="code"> {{ item.Code }} </td>
                                            <td class="product_name"> {{ item.ProductName }} </td>
                                            <td> <input type="number" min="1" class="col-md-12 quantaty" ng-model="item.Quantity" ng-blur="onChangeQuanity(item)" ng-change="onChangeQuanity(item)" /> </td>
                                            <td class="origin_price"> {{ item.OriginPriceStr }} </td>
                                            <td class="price"> {{ item.PriceStr }} </td>
                                            <td>  <input type="text" class="full-width note" ng-model="item.Note" placeholder="Nhập ghi chú" /></td>
                                            <td class="text-center">
                                                <a href="javascript:void(0)" ng-click="decreaseProduct(item)" class="btn btn-danger btn-sm" data-toggle="tooltip" data-placement="top" title="Xóa khỏi giỏ hàng"><i class="fa fa-trash-o"></i> </a>
                                            </td>
                                        </tr>

                                    </tbody>
                                </table>
                            </div>
                            <div>
                                <div class="ln_solid" style="border-bottom: 1px solid #f39c12; margin: 5px;"></div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="col-lg-8 col-md-8 col-xs-8">
                                            <span class="label-order mg-right-5"><i class="fa fa-money"></i> Phí ship : </span>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-xs-4">
                                            <label>{{Order.ShipFeeStr}} đ</label>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class="col-lg-8 col-md-8 col-xs-8">
                                            <span class="label-order mg-right-5"><i class="fa fa-money"></i> Tổng tiền : </span>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-xs-4">
                                            <label>{{Order.OrderOriginPriceStr}} đ</label>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class="col-lg-8 col-md-8 col-xs-8">
                                            <span class="label-order mg-right-5"><i class="fa fa-money"></i> Khuyến mại : </span>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-xs-4">
                                            <label>{{ DiscountStr }} đ</label>
                                        </div>
                                    </div>
                                    <div class="col-md-12">
                                        <div class="col-lg-8 col-md-8 col-xs-8">
                                            <span class="label-order mg-right-5"><i class="fa fa-money"></i> Thành tiền : </span>
                                        </div>
                                        <div class="col-lg-4 col-md-4 col-xs-4">
                                            <label>{{Order.OrderPriceStr}} đ</label>
                                        </div>
                                    </div>
                                    @*<label>Tổng tiền:</label><label id="totalPrice">{{Order.OrderOriginPriceStr}}</label>
                                        <label>Khuyến mại:</label><label id="discount"></label>
                                        <label>Thành tiền:</label><label id="lastPrice">{{Order.OrderPriceStr}}</label>*@
                                </div>
                            </div>
                            <input type="hidden" id="hddDiscountValue" value="@DVG.WIS.Utilities.AppSettings.Instance.GetString("DiscountValuePercent")" />
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <div class="pull-right" style="position:fixed; right: 20px; bottom: 2px; z-index: 99">
            <button type="button" class="btn btn-primary mg-right-10" onclick="return validate()" data-ng-disabled="frmUpdateOrder.$invalid || Order.OrderType < 0 || IsErrorPhone || IsErrorDeliveryDate || Order.ListProductViewModel == null || Order.ListProductViewModel == [] || Order.ListProductViewModel.length == 0 || (Order.OrderType <= 0 && (Order.DeliveryAddress == ''|| Order.DistrictId <= 0))" ng-click="doUpdate(@((int)OrderStatusEnum.Inactive))">
                <i class="fa fa-save"></i>
                <span ng-if="Order.OrderId == 0">Tạo đơn</span>
                <span ng-if="Order.OrderId > 0">Sửa đơn</span>
            </button>
            <button type="button" class="btn btn-primary mg-right-10" onclick="return validate()" data-ng-disabled="frmUpdateOrder.$invalid || Order.OrderType < 0 || IsErrorPhone || IsErrorDeliveryDate || Order.ListProductViewModel == null || Order.ListProductViewModel == [] || Order.ListProductViewModel.length == 0 || (Order.OrderType <= 0 && (Order.DeliveryAddress == '' || Order.DistrictId <= 0)) " ng-click="doUpdate(@((int)OrderStatusEnum.Pending))">
                <i class="fa fa-save"></i>
                <span ng-if="Order.OrderId == 0">Tạo và duyệt</span>
                <span ng-if="Order.OrderId > 0">Sửa và duyệt</span>
            </button>
            <a class="btn btn-info w-90 mg-right-10" onclick="return confirmcancel();"><i class="fa fa-remove"></i> Hủy</a>
        </div>
    </form>

